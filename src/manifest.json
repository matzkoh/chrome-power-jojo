{
  "name": "Power JoJo",
  "description": "Activate Manga Sound Effect like JoJo",
  "version": "1.1.0",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "css": ["content.css"],
      "js": ["store.js", "content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": ["storage"],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "web_accessible_resources": ["shadow.css", "fonts/*", "images/*"],
  "manifest_version": 2
}
